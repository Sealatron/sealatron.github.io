	"Post Front Matter": {
    "prefix": "front",
    "description": "Create YAML Front Matter for Jekyll site",
    "body": [
		"---",
		"layout: post",
        "title: ${1:title}",
        "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND",
		"---"
    ]
}
}